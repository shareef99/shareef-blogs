{
    "name": "shareef-blogs",
    "displayName": "shareef-blogs",
    "description": "A vs-code extension to search blogs of shareef.",
    "version": "0.0.1",
    "publisher": "NadeemShareef",
    "repository": {
        "type": "git",
        "url": "https://github.com/shareef99/show-time"
    },
    "icon": "shareef blog.jpg",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:shareef-blogs.searchShareefBlogs"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "shareef-blogs.searchShareefBlogs",
                "title": "Search Shareef Blogs"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "@types/vscode": "^1.60.0",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "axios": "^0.21.1"
    }
}
